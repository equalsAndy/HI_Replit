    \
    // \uc0\u55357 \u56613  Workshop completion: Keep 4 & 5 expanded, collapse 3\
    if (workshopCompleted) \{\
      expansion['1'] = false;\
      expansion['2'] = false;\
      expansion['3'] = false; // COLLAPSE Section 3\
      expansion['4'] = true;  // KEEP Section 4 expanded\
      expansion['5'] = true;  // KEEP Section 5 expanded\
      expansion['6'] = true;  // Accessible\
