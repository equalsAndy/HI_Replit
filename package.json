{
  "name": "rest-express",
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:hmr": "NODE_ENV=development tsx dev-server.js",
    "dev:clean": "bash scripts/run_dev_hmr.sh",
    "dev:ports": "lsof -ti tcp:8080,24679 || echo 'Ports 8080 and 24679 are free'",
    "build": "./update-version.sh && cd client && vite build && mkdir -p ../dist/public/assets && cp -a public/assets/* ../dist/public/assets/ && cd .. && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && node scripts/bundle-size-check.js || true",
    "build:staging": "./version-manager.sh staging patch && cd client && vite build && mkdir -p ../dist/public/assets && cp -a public/assets/* ../dist/public/assets/ && cd .. && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:staging:minor": "./version-manager.sh staging minor && cd client && vite build && mkdir -p ../dist/public/assets && cp -a public/assets/* ../dist/public/assets/ && cd .. && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:staging:major": "./version-manager.sh staging major && cd client && vite build && mkdir -p ../dist/public/assets && cp -a public/assets/* ../dist/public/assets/ && cd .. && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:production": "./version-manager.sh production && cd client && vite build && mkdir -p ../dist/public/assets && cp -a public/assets/* ../dist/public/assets/ && cd .. && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "start:development": "NODE_ENV=development node dist/index.js",
    "preview": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "db:push": "drizzle-kit push",
    "version:update": "./update-version.sh",
    "version:patch": "./update-version.sh patch",
    "version:minor": "./update-version.sh minor",
    "version:major": "./update-version.sh major",
    "bundle:analyze": "node scripts/bundle-size-check.js",
    "bundle:check": "npm run build && node scripts/bundle-size-check.js",
    "danger:backup-delete-users": "USER_IDS=2-11,17-28,31-32 node scripts/backup-and-delete-users.cjs",
    "danger:backup-only": "DRY_RUN=1 USER_IDS=2-11,17-28,31-32 node scripts/backup-and-delete-users.cjs",
    "danger:backup": "npm run danger:backup-only",
    "danger": "npm run danger:backup-delete-users"
  },
